package constructor;

public class book {
	//按眉 积己
	private String title;
	private String author;
	private int page;
	
	//叼弃飘 积己磊
	public book() {

	}
	
	//getter, setter
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getAuthor() {
		return author;
	}
	public void setAuthor(String author) {
		this.author = author;
	}
	public int getPage() {
		return page;
	}
	public void setPage(int page) {
		this.page = page;
	}
	//toString
	

}
