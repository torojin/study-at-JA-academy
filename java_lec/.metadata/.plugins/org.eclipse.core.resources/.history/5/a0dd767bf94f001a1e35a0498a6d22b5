package weekendstudy;

public class Family {
	//field(variable) »ý¼º
	private String name;
	
	//default constructor
	public Family() {
		super();
	}
	//parameter constructor
	public Family(String name) {
		super();
		this.name=name;
	}
	//toString
	public String toString() {
		return ""+name;
	}

}
