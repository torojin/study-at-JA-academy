package staticex;

public class BankAccount {
	//field 생성
	private int year;
	private String name;
	private int money;
	private static double rate;
	public BankAccount(int year, String name, int money, double r) {
		super();
		this.year = year;
		this.name = name;
		this.money = money;
		rate=r;
	}
	public static void setRate(double r) {// static 변수를 제어하는 함수
		//유효성 검사를 위해 사용
	}
	
}
